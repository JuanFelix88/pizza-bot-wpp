{
  "name": "escorts-api",
  "version": "0.1.0",
  "main": "dist/server.js",
  "engines": {
    "node": ">=14"
  },
  "scripts": {
    "dev": "ts-node-dev -r tsconfig-paths/register --transpile-only src/app.ts",
    "poc": "ts-node-dev -r tsconfig-paths/register --transpile-only test.ts",
    "test": "ts-node-dev -r tsconfig-paths/register --transpile-only src/server.ts",
    "build": "babel src --extensions \".js,.ts\" --out-dir dist  --copy-files --no-copy-ignored",
    "start": "node dist/server.js",
    "box": "ts-node-dev --transpile-only  sandbox.ts",
    "sand": "node sandbox.js"
  },
  "license": "MIT",
  "dependencies": {
    "chalk": "^4.1.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "venom-bot": "^3.0.17"
  },
  "devDependencies": {
    "@babel/cli": "^7.10.3",
    "@babel/core": "^7.10.3",
    "@babel/node": "^7.10.3",
    "@babel/plugin-transform-typescript": "^7.11.0",
    "@babel/preset-env": "^7.11.5",
    "@babel/preset-typescript": "^7.10.4",
    "@types/exceljs": "^1.3.0",
    "@types/express": "^4.17.13",
    "@types/socket.io": "^2.1.12",
    "@typescript-eslint/eslint-plugin": "^4.15.0",
    "@typescript-eslint/parser": "^4.15.0",
    "babel-loader": "^8.0.6",
    "babel-plugin-module-resolver": "^4.0.0",
    "eslint": "^7.19.0",
    "eslint-config-standard": "^16.0.2",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "source-map-loader": "^0.2.4",
    "ts-node-dev": "^1.1.1",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^4.1.3"
  }
}
